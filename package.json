    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@dotlottie/react-player": "^1.6.16",
    "@lottiefiles/react-lottie-player": "^3.5.3",
